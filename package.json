{
  "name": "@jcrengifoa/monkeyrepo",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "app:build": "tsc && vite build",
    "app:clean": "rm -rf dist && rm -rf node_modules",
    "app:develop": "vite",
    "app:preview": "vite preview",
    "packages:build": "lerna bootstrap && lerna run build",
    "packages:clean": "lerna run clean",
    "packages:test": "lerna run test",
    "packages:publish": "lerna publish",
    "storybook:develop": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "chromatic": "from-env chromatic --project-token %CHROMATIC_PROJECT_TOKEN --build-script-name storybook:build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-sass": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/blocks": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/react-vite": "^7.0.18",
    "@storybook/testing-library": "^0.1.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "chromatic": "^6.18.0",
    "from-env": "^1.1.4",
    "lerna": "^6.6.2",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.18",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=18.16.0"
  }
}
